{
  "name": "neo-client",
  "productName": "neoChat",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "electron": "vue-cli-service build && electron .",
    "build-osx": "vue-cli-service build && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/AppIcon.icns --prune=true --out=build",
    "build-win": "vue-cli-service build && electron-packager . neoChat --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/icon.ico --prune=true --version-string.CompanyName=\"FESTIVAL Development\" --version-string.FileDescription=\"neoChat Desktop Application\" --version-string.ProductName=\"neoChat\""
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "metroui-vue": "file:metroui-vue",
    "node-rsa": "^1.0.2",
    "vue": "^2.5.17",
    "vue-headful": "^2.0.1",
    "vue-router": "^3.0.1",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "electron": "^4.0.6",
    "electron-default-menu": "^1.0.1",
    "electron-packager": "^13.1.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
